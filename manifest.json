{
    "name": "ETHelper",
    "version": "1.0.0",
    "description": "Remix documentation search",
    "permissions": [
        "storage",
        "tabs",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "action": {
        "default_title": "ETHelper",
        "default_popup": "index.html"
      },
    "manifest_version": 3
}