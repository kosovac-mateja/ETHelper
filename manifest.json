{
    "name": "ETHelper",
    "version": "1.0.0",
    "description": "Remix documentation search",
    "permissions": [
        "storage",
        "tabs",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://remix.ethereum.org/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "manifest_version": 3
}